		<defaultValueExpression ><![CDATA[($P{diseaseArea}.equals( "'All'" ) ? "'%'" : ("'" + $P{diseaseArea}.substring(1,($P{diseaseArea}.length()-1)).replaceAll("'", "''") + "'"))]]></defaultValueExpression>
	</parameter>
	<parameter name="finalAreaOfExpertise" isForPrompting="false" class="java.lang.String">
		<defaultValueExpression ><![CDATA[($P{areaOfExpertise}.equals( "'%'" ) ? "'%'" : ("'" + $P{areaOfExpertise}.substring(1,($P{areaOfExpertise}.length()-1)).replaceAll("'", "''") + "'"))
]]></defaultValueExpression>
	</parameter>
	<queryString><![CDATA[select ol.*, soi.value as Sphere_of_influence, 
AFFI_ATTR.ta as ol_ta,
decode(author.value, 'N/A', '', author.value ) as author_flag,
decode(investigator.value, 'N/A', '', investigator.value ) as investigator_flag, 
decode(advisor.value, 'N/A', '', advisor.value ) as advisor_flag,
decode(speaker.value, 'N/A', '', speaker.value ) as speaker_flag,
da1.value as diseaseArea1, 
da2.value as diseaseArea2, da3.value as diseaseArea3, 
aoe1.value as AreaOfExpertise1, aoe2.value as AreaOfExpertise2, 
aoe3.value as AreaOfExpertise3,
(select value from global_constants where name = 'OBJECTIVETEXT_BMS_POTENTIAL_EXPERTS' ) as OBJECTIVE_TEXT
from hq_ol_view ol
left outer join attribute_value_view da1
on (da1.root_entity_id = ol.kolid and da1.column_name = 'Disease Area 1')
left outer join attribute_value_view da2
on (da2.root_entity_id = ol.kolid and da2.column_name = 'Disease Area 2')
left outer join attribute_value_view da3
on (da3.root_entity_id = ol.kolid and da3.column_name = 'Disease Area 3')
left outer join attribute_value_view aoe1
on (aoe1.root_entity_id = ol.kolid and aoe1.column_name = 'Area of Expertise 1')
left outer join attribute_value_view aoe2
on (aoe2.root_entity_id = ol.kolid and aoe2.column_name = 'Area of Expertise 2')
left outer join attribute_value_view aoe3
on (aoe3.root_entity_id = ol.kolid and aoe3.column_name = 'Area of Expertise 3')
left outer join hq_ol_soi_view soi
on (soi.root_entity_id = ol.kolid)
left outer join HQ_OL_AFFILIATION_VIEW AFFI_ATTR 
on ( OL.kolid = AFFI_ATTR.root_entity_id )
left outer join hq_ol_author_view author
on (author.root_entity_id = AFFI_ATTR.root_entity_id and author.ta = AFFI_ATTR.ta)
left outer join hq_ol_advisor_view advisor
on (advisor.root_entity_id = AFFI_ATTR.root_entity_id and advisor.ta = AFFI_ATTR.ta)
left outer join hq_ol_speaker_view speaker
on (speaker.root_entity_id = AFFI_ATTR.root_entity_id and speaker.ta = AFFI_ATTR.ta)
left outer join hq_ol_investigator_view investigator
on (investigator.root_entity_id = AFFI_ATTR.root_entity_id and investigator.ta = AFFI_ATTR.ta)
where 
((da1.value like ($P!{finalDiseaseArea}) and aoe1.value like ($P!{finalAreaOfExpertise})) 
or (da2.value like ($P!{finalDiseaseArea}) and aoe2.value like ($P!{finalAreaOfExpertise}))
or (da3.value like ($P!{finalDiseaseArea}) and aoe3.value like ($P!{finalAreaOfExpertise})))
and (
( da1.value like ($P!{finalDiseaseArea}) and aoe1.value like ($P!{finalAreaOfExpertise})
and da1.value in (select optvalue from option_lookup where option_id in 
(select id from option_names where name like 'Disease Area')
and id not in
(select feature_id from feature_usergroup_map where permission_on_feature = 2 
  and usergroup_id 
  like '%,'||(select group_id from user_group_map where user_id in ($P!{userId}))||',%')
) and aoe1.value in 
(select optValue from option_lookup where option_id = 
(select id from option_names where name like 'Area of Expertise')
and parent in (select id from option_lookup where optvalue = da1.value
  and option_id = (select id from option_names where name like 'Disease Area'))
and id not in 
(select feature_id from feature_usergroup_map where permission_on_feature = 2 
  and usergroup_id 
  like '%,'||(select group_id from user_group_map where user_id in ($P!{userId}))||',%'))
) or
( da2.value like ($P!{finalDiseaseArea}) and aoe2.value like ($P!{finalAreaOfExpertise})
and da2.value in (select optvalue from option_lookup where option_id in 
(select id from option_names where name like 'Disease Area')
and id not in
(select feature_id from feature_usergroup_map where permission_on_feature = 2 
  and usergroup_id 
  like '%,'||(select group_id from user_group_map where user_id in ($P!{userId}))||',%')
) and aoe2.value in 
(select optValue from option_lookup where option_id = 
(select id from option_names where name like 'Area of Expertise')
and parent in (select id from option_lookup where optvalue = da2.value
  and option_id = (select id from option_names where name like 'Disease Area'))
and id not in 
(select feature_id from feature_usergroup_map where permission_on_feature = 2 
  and usergroup_id 
  like '%,'||(select group_id from user_group_map where user_id in ($P!{userId}))||',%'))
) or
( da3.value like ($P!{finalDiseaseArea}) and aoe3.value like ($P!{finalAreaOfExpertise})
and da3.value in (select optvalue from option_lookup where option_id in 
(select id from option_names where name like 'Disease Area')
and id not in
(select feature_id from feature_usergroup_map where permission_on_feature = 2 
  and usergroup_id 
  like '%,'||(select group_id from user_group_map where user_id in ($P!{userId}))||',%')
) and aoe3.value in 
(select optValue from option_lookup where option_id = 
(select id from option_names where name like 'Area of Expertise')
and parent in (select id from option_lookup where optvalue = da3.value
  and option_id = (select id from option_names where name like 'Disease Area'))
and id not in 
(select feature_id from feature_usergroup_map where permission_on_feature = 2 
  and usergroup_id 
  like '%,'||(select group_id from user_group_map where user_id in ($P!{userId}))||',%'))
))
order by lower(AFFI_ATTR.ta), lower(ol.ollastname), lower(ol.olfirstname)]]></queryString>

