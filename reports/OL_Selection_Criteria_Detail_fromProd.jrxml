    o1.optvalue as region,
    o2.optvalue as TA,
    u.lastname||', '||u.firstname name,
    u2.lastname||', '||u2.firstname RegionUser
    from user_table u,user_table u1,contacts c,
    user_relationship ur,
    user_relationship ur1,
    user_table u2,
    option_lookup o,
