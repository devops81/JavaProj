	<queryString><![CDATA[select terr.territoryID, optl.optValue as territoryName,optl2.optValue as regionName, optl3.optValue as TAName,
terr.territoryCount as territoryCount, terr.userDetails as userDetails
from
	(select urv.territory as territoryID, count(urv.user_id) as territoryCount, stragg(urv.name) as userDetails  from
		(select ur.territory, ur.user_id,(ut.id || '-' || ut.lastname ||' '|| ut.firstname) as name from user_relationship ur
			join user_table ut
			on (ut.id = ur.user_id)
			where ut.deleteflag = 'Y'
			and ur.begin_date <= sysdate
			and ur.end_date >= sysdate
			and ur.relationship_type = 1) urv
		group by urv.territory
	) terr
left outer join option_lookup optl
on (optl.id = terr.territoryID)
left outer join option_lookup optl2
on (optl2.id = optl.parent)
left outer join option_lookup optl3
on (optl3.id = optl2.parent)]]></queryString>

