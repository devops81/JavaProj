	</parameter>
	<queryString><![CDATA[select optl.id as TerritoryID, optl.optValue as TerritoryName,
optl2.optValue as RegionName, optl3.optValue as TAName
from option_lookup optl 
left outer join option_lookup optl2
on (optl2.id = optl.parent)
left outer join option_lookup optl3
on (optl3.id = optl2.parent)
where optl.option_id = 
(select id from option_names where name = 'Territory')
and optl.id not in
(select ur.territory from user_relationship ur 
inner join user_table ut on (ut.id = ur.user_id)
where ur.begin_date <= sysdate 
and ur.end_date >= sysdate
and ut.deleteflag = 'N')]]></queryString>

