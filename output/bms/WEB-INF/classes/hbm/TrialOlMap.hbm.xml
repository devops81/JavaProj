<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 2.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-2.0.dtd">

<hibernate-mapping>

    <class name="com.openq.eav.trials.TrialOlMap" table="TRIAL_OL_MAP">
        <id name="id" column="id">
            <generator class="native"/>
        </id>
    
        <property name="trialId" type="long"/>
        <property name="olId" type="long"/>
        <property name="role" />
        <property name="institution"/>
        
     </class>
     
</hibernate-mapping>