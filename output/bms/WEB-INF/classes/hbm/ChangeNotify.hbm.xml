<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 2.0//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-2.0.dtd">

<hibernate-mapping>
	<class name="com.openq.Notify.ChangeNotify" table="CHANGE_NOTIFY_TABLE">
		<id name="id" column="ID">
			<generator class="native"/>
		</id>
		<property name="kolId" column="KOLID" type="long"  />
		<property name="attribute_id" column="ATTRIBUTE_ID"  type="long"/>
		<property name="attribute" column="attribute" type="long" />
		<property name="original_value" column="ORIGINAL_VALUE"  />
		<property name="new_value" column="NEW_VALUE"  />
		<property name="operation" column="OPERATION" type="long" />
		<property name="created_at" column="CREATED_AT" type="date" />
		<property name="updated_at" column="UPDATED_AT" type="date" />


	</class>
</hibernate-mapping>