<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 2.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-2.0.dtd">

<hibernate-mapping>

    <class name="com.openq.time.TimeReport" table="time_report">
        <id name="id" column="id">
            <generator class="native"/>
        </id>	
        <property name="userId" type="long"/>
        <property name="activityDate" type="date"/>
        <property name="activityId" type="long"/>
        <property name="percentage" type="long"/>       
        <property name="comments" type="string"/>
        <property name="finishedFlag" type="string"/>
        <property name="createDate" type="date"/>        
        <property name="updateDate" type="date"/>
        <property name="deleteFlag" type="string"/>
     </class>
</hibernate-mapping>