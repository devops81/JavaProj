<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 2.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-2.0.dtd">

<hibernate-mapping>

    <class name="com.openq.authorization.FeaturePermissionMetadata" table="FEATURE_PERMISSION_METADATA">
        <id name="id" column="ID">
            <generator class="native"/>
        </id>
		<property name="permissionName" column="PERMISSION_NAME"/>
		<property name="allowedByDefault" column="ALLOWED_BY_DEFAULT"/>
     </class>
     
</hibernate-mapping>    
