<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 2.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-2.0.dtd">

<hibernate-mapping>

    <class name="com.openq.ig.pubmed.crawler.keyword.KeywordCategory" table="keyword_categories">  
		<id name="id" >
            <generator class="native"/>
        </id>

	 <property name="category" column="category"/>
     <property name="keywords" column="keywords"/>
	 <property name="status"  column="status"/>
     <property name="createtime" column="createtime" type="date"/>
	 <property name="lastRuntime" column="lastruntime" type="date"/>
     </class>

   
</hibernate-mapping>