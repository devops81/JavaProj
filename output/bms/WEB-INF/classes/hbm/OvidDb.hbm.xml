<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 2.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-2.0.dtd">

<hibernate-mapping>

    <class name="com.openq.publication.data.OvidDb" table="ovid_datasource">
        <id name="id" >
        	<generator class="native"/>
        </id>
        <property name="ovidDatabaseName" column="oviddb" />
        <property name="ovidDatabaseNameFull" column="oviddbfull" />
     </class>
</hibernate-mapping>