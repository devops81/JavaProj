<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 2.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-2.0.dtd">

<hibernate-mapping>

    <class name="com.openq.authorization.FeatureUsergroupMap" table="FEATURE_USERGROUP_MAP">
        <id name="id" column="ID">
            <generator class="native"/>
        </id>
		<property name="featureId" column="FEATURE_ID" type="long"/>
		<property name="usergroupId" column="USERGROUP_ID"/>
		<property name="permissionOnFeature" column="PERMISSION_ON_FEATURE" type="long"/>
     </class>
     
</hibernate-mapping>    
