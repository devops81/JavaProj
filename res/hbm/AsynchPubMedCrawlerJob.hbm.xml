<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 2.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-2.0.dtd">

<hibernate-mapping>
    <class name="com.openq.pubmed.AsynchPubMedCrawlerJob" table="asynch_pubmed_crawler_job">  
        <id name="id" column="id">
            <generator class="native"/>
        </id>
        <property name="scheduledTime" column="scheduled_time" type="long"/>
        <property name="status" column="status"/>
        <property name="totalOLCount" column="total_ol_count" type="long"/>
     </class>  
</hibernate-mapping>    
