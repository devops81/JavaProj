<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 2.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-2.0.dtd">

<hibernate-mapping>

    <class name="com.openq.event.materials.EventMaterial" table="EVENT_MATERIAL">
        <id name="materialID" column="material_id">
            <generator class="native"/>
        </id>
        <property name="eventId" column="event_id"/>
        <property name="name"/>
        <property name="description"/>
        <property name="materialContent" column="material_content" type="blob"/>
        
     </class>
     
</hibernate-mapping>   