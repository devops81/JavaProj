<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 2.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-2.0.dtd">

<hibernate-mapping>

    <class name="com.openq.user.HomeUserAddressView" table="Home_User_Address_View">
        <id name="uniqueId" column="unq_id">
            <generator class="native"/>
        </id>
        <property name="id" type="long" />
        <property name="kolId" type="long" />
        <property name="city" type="string" />
        <property name="state" type="string" />
        <property name="line1" column="ADDR_LINE1" type="string" />
        <property name="line2" column="ADDR_LINE2" type="string" />
        <property name="country" type="string" />
		<property name="usage" type="string" />
		<property name="type" type="string" />
		<property name="zip" type="string" />
		
     </class>

</hibernate-mapping>
