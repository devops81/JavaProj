<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 2.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-2.0.dtd">

<hibernate-mapping>

    <class name="com.openq.interactionData.UserRelationship" table="USER_RELATIONSHIP">
        <id name="id" column="id">
            <generator class="native"/>
        </id>
        <property name="userId" column="USER_ID"  />
        <property name="supervisorId" column="RELATED_USER_ID" />
        <property name="reportLevel" column="RELATIONSHIP_TYPE"  />
        <property name="beginDate" column="BEGIN_DATE" type="date" />
        <property name="endDate" column="END_DATE" type="date" />
        <property name="territory" column="TERRITORY"  />
    </class>
     
</hibernate-mapping>  
