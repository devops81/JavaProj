<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 2.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-2.0.dtd">

<hibernate-mapping>

    <class name="com.openq.user.SpecialtyView" table="specialty_view">
        <id name="id" column="id">
            <generator class="native"/>
        </id>
        <property name="firstname"  type="string"/>
        <property name="middlename"  type="string"/>
        <property name="lastname"  type="string"/>
        <property name="deleteflag"  type="string"/>
        <property name="kolid" type="long" />
        <property name="specialty1" type="string" />
        <property name="specialty2" type="string" />
        <property name="specialty3" type="string" />
       <!--<property name="specialty4" type="string" />
        <property name="specialty5" type="string" />
        <property name="specialty6" type="string" />
        -->
     </class>

</hibernate-mapping>
